{
    "name": "n8n-nodes-digichat",
    "version": "1.0.15",
    "description": "Send WhatsApp messages via digiChat API in n8n workflows",
    "main": "dist/nodes/DigiChatNode.node.js",
    "types": "dist/nodes/DigiChatNode.node.d.ts",
    "scripts": {
        "build": "tsc && mkdir -p dist/nodes/icons && cp src/nodes/icons/digichat.svg dist/nodes/icons/",
        "dev": "npx tsc --watch",
        "clean": "rm -rf dist"
    },
    "keywords": [
        "n8n-community-node-package",
        "whatsapp",
        "digiChat",
        "messaging",
        "api"
    ],
    "license": "MIT",
    "homepage": "https://github.com/Inferno-Team/n8n-nodes-digichat#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/Inferno-Team/n8n-nodes-digichat.git"
    },
    "bugs": {
        "url": "https://github.com/Inferno-Team/n8n-nodes-digichat/issues"
    },
    "author": {
        "name": "DigiWorld-Team",
        "email": "m.alissa@digiworld-dev.com"
    },
    "files": [
        "dist/nodes/**/*",
        "dist/credentials/**/*",
        "dist/icons/**/*"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "nodes": [
            "dist/nodes/DigiChatNode.node.js"
        ],
        "credentials": [
            "dist/credentials/DigiChatApi.credentials.js"
        ]
    },
    "devDependencies": {
        "@types/jest": "^29.5.0",
        "@types/node": "^18.15.11",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.45.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "gulp": "^4.0.2",
        "jest": "^29.5.0",
        "n8n-core": "^1.0.0",
        "n8n-workflow": "^1.0.0",
        "prettier": "^3.0.0",
        "ts-jest": "^29.1.0",
        "typescript": "^5.8.3"
    },
    "dependencies": {
        "n8n-core": "^1.0.0",
        "n8n-workflow": "^1.0.0"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    }
}
